---
import experiences from '@/data/experiences.json';
import experience_configs from '@/data/experience_configs.json';

import SimpleHeader from '@/components/SimpleHeader.astro';
import InfoList from '@/components/InfoList.astro';
---

<div class="w-[90vw] md:w-[55vw]">
	<SimpleHeader text="Experiences" number="ii" />
	<InfoList 
		items={experiences}  
		num_show={experience_configs.num_show}
		id='experiences'
	/>
</div>

