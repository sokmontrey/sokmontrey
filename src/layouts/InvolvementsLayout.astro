---
import involvements from '@/data/involvements.json';
import involvement_configs from '@/data/involvement_configs.json';

import SimpleHeader from '@/components/SimpleHeader.astro';
import InfoList from '@/components/InfoList.astro';
---

<div class="w-[90vw] md:w-[55vw]">
	<SimpleHeader text="Community Involvements" number="iii" />
	<InfoList 
		items={involvements}  
		num_show={involvement_configs.num_show}
		id='involvements'
	/>
</div>

